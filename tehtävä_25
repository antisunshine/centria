#include <iostream>
#include <time.h>

using namespace std;

int minimi(int num1, int num2);

int main() {

    int eka, toka;
    cout << "Give 1. number:" << endl;
    cin >> eka;
    cout << "Give 2. number:" << endl;
    cin >> toka;
    cout << "The smoll number is : " << minimi(eka,toka);

  return 0;
}

int minimi(int num1, int num2){
    int smoll;
    if (num1 > num2){
        smoll = num2;
  } else {
    smoll = num1;
  }
  return smoll;
}
