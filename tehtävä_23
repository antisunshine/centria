/*************************************************
* Ohjelma tekee funktion joka tulostaa halutun   *
* määrän tähtiä.                                 *
* Susanne Koljonen                               *
************************************************/

#include <iostream>

using namespace std;

void starFunction(int num){
  for(int i = 0; i < num; i++){
    cout << "*"; 
  }
}

int main() {
  starFunction(10);
}
